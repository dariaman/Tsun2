CASE WHEN CHARINDEX('and/or', PLC.PLCTTG) &gt; 0 THEN SUBSTRING(PLC.PLCTTG, 0, CHARINDEX('and/or', PLC.PLCTTG)) ELSE PLC.PLCTTG END AS TTG, 
  P.MTU, P.DUEAMOUNT-P.PAYMENT_OC AS DUEAMOUNT, DATEDIFF(DAY, NT.ADATE, GETDATE()) AS DAYS, ADD2.VAL AS PICMKT, CO.KODECO, P.ISPAID, P.PAYMENT_OC, NT.KODETOC,
  CASE WHEN NT.ADATE &gt; GETDATE() THEN P.DUEAMOUNT - P.PAYMENT_OC ELSE 0 END AS NOTDUE,
  CASE WHEN DATEDIFF(DAY, NT.ADATE, GETDATE()) BETWEEN 0 AND 30 THEN P.DUEAMOUNT-P.PAYMENT_OC ELSE 0 END AS DAYS30,
  CASE WHEN DATEDIFF(DAY, NT.ADATE, GETDATE()) BETWEEN 31 AND 60 THEN P.DUEAMOUNT-P.PAYMENT_OC ELSE 0 END AS DAYS60,
  CASE WHEN DATEDIFF(DAY, NT.ADATE, GETDATE()) BETWEEN 61 AND 90 THEN P.DUEAMOUNT-P.PAYMENT_OC ELSE 0 END AS DAYS90,
  CASE WHEN DATEDIFF(DAY, NT.ADATE, GETDATE()) &gt; 90 THEN P.DUEAMOUNT-P.PAYMENT_OC ELSE 0 END AS DAYS91,
  ISNULL(ADD1_DNV.VAL, ADD2_DNV.VAL) AS DN_PLCSYS1, PLC.PLCID, NT.NOTANO
FROM FAPAYMENT P
INNER JOIN FAVOUCHER VO ON VO.VOUCHER = P.VOUCHER
INNER JOIN PLCNOTA NT ON VO.NOTANO = NT.NOTANO
INNER JOIN PLACING PLC ON PLC.PLCID = NT.PLCID
INNER JOIN PLACINGTOC PT ON PT.PLCID = PLC.PLCID AND NT.KODETOC = PT.KODETOC
LEFT OUTER JOIN PLCINSURANCE INS ON NT.PLCID = INS.PLCID AND INS.TYPEF = 'L'
LEFT OUTER JOIN PLCINSURANCE MINS ON PLC.MPLCID = MINS.PLCID AND MINS.TYPEF = 'L'
LEFT OUTER JOIN PLCADDINFO ADD1 ON NT.PLCID = ADD1.PLCID AND ADD1.CONFIGID = 'POLICY_NO'
LEFT OUTER JOIN PLCADDINFO ADD2 ON NT.PLCID = ADD2.PLCID AND ADD2.CONFIGID = 'PS_PIC_MKT'
LEFT OUTER JOIN CO ON CO.USRID = ADD2.VAL
LEFT OUTER JOIN PLCADDINFO ADD1_DNV ON NT.PLCID = ADD1_DNV.PLCID AND ADD1_DNV.CONFIGID = 'DN_VISION' AND PT.URUT = 1
LEFT OUTER JOIN PLCADDINFO ADD2_DNV ON NT.PLCID = ADD2_DNV.PLCID AND ADD2_DNV.CONFIGID = 'DN_VISION2'  AND PT.URUT = 2
WHERE NT.DCTYPE IN ('BB', 'BF')
AND (NT.NAMA LIKE @TTG) AND (NT.ADATE BETWEEN @SDATE AND @EDATE)
AND (PLC.KODECAB LIKE @KODECAB) AND (P.ISPAID = 0) 
AND (ISNULL(ADD1_DNV.VAL, ADD2_DNV.VAL) LIKE @DNVISION1 OR '1' = @ISDNVISION1)
AND (NT.NOTANO LIKE @NOTANO OR '1' = @ISNOTANO)
AND (CO.KODECO LIKE @KODECO)
ORDER BY P.VOUCHER</CommandText>
