            <Sources>
              <DbSource ConnectionRef="FAConnectionString (Settings)" DbObjectName="TALYS.dbo.FAPAYMENT" DbObjectType="Table" GenerateMethods="Get" GenerateShortCommands="true" GeneratorGetMethodName="GetDataByFilterAgingNote" GetMethodModifier="Public" GetMethodName="GetDataByFilterAgingNote" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="true" UserGetMethodName="GetDataByFilterAgingNote" UserSourceName="GetDataByFilterAgingNote">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT P.MTU, 
  SUM(P.DUEAMOUNT) AS DUEAMOUNT, 
  SUM(CASE WHEN NT.ADATE &gt; GETDATE() THEN P.DUEAMOUNT- P.PAYMENT_OC ELSE 0 END) AS NOTDUE,
  SUM(CASE WHEN DATEDIFF(DAY, NT.ADATE, GETDATE()) BETWEEN 0 AND 30 THEN P.DUEAMOUNT-P.PAYMENT_OC ELSE 0 END) AS DAYS30,
  SUM(CASE WHEN DATEDIFF(DAY, NT.ADATE, GETDATE()) BETWEEN 31 AND 60 THEN P.DUEAMOUNT-P.PAYMENT_OC ELSE 0 END) AS DAYS60,
  SUM(CASE WHEN DATEDIFF(DAY, NT.ADATE, GETDATE()) BETWEEN 61 AND 90 THEN P.DUEAMOUNT-P.PAYMENT_OC ELSE 0 END) AS DAYS90,
  SUM(CASE WHEN DATEDIFF(DAY, NT.ADATE, GETDATE()) &gt; 90 THEN P.DUEAMOUNT-P.PAYMENT_OC  ELSE 0 END) AS DAYS91
FROM FAPAYMENT P
INNER JOIN FAVOUCHER VO ON VO.VOUCHER = P.VOUCHER
INNER JOIN PLCNOTA NT ON VO.NOTANO = NT.NOTANO
INNER JOIN PLACING PLC ON PLC.PLCID = NT.PLCID
INNER JOIN PLACINGTOC PT ON PT.PLCID = PLC.PLCID AND NT.KODETOC = PT.KODETOC
LEFT OUTER JOIN PLCADDINFO ADD1_DNV ON NT.PLCID = ADD1_DNV.PLCID AND ADD1_DNV.CONFIGID = 'DN_VISION' AND PT.URUT = 1
LEFT OUTER JOIN PLCADDINFO ADD2_DNV ON NT.PLCID = ADD2_DNV.PLCID AND ADD2_DNV.CONFIGID = 'DN_VISION2'  AND PT.URUT = 2
LEFT OUTER JOIN PLCADDINFO ADD2 ON NT.PLCID = ADD2.PLCID AND ADD2.CONFIGID = 'PS_PIC_MKT'
LEFT OUTER JOIN CO ON CO.USRID = ADD2.VAL
WHERE (NT.DCTYPE IN ('BB', 'BF') OR (NT.DCTYPE = 'OP' AND NT.DC = 'D')) 
AND (P.ISPAID = 0) 
AND (NT.NAMA LIKE @TTG) AND (NT.ADATE BETWEEN @SDATE AND @EDATE) AND (PLC.KODECAB LIKE @KODECAB) 
AND (ISNULL(ADD1_DNV.VAL, ADD2_DNV.VAL) LIKE @DNVISION1 OR '1' = @ISDNVISION1)
AND (NT.NOTANO LIKE @NOTANO OR '1' = @ISNOTANO)
AND (CO.KODECO LIKE @KODECO)
GROUP BY P.MTU</CommandText>
