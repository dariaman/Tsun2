  CASE WHEN DATEDIFF(DAY, P.DUEDATE, GETDATE()) BETWEEN 31 AND 60 THEN P.DUEAMOUNT- P.PAYMENT_OC ELSE 0 END AS DAYS60,
  CASE WHEN DATEDIFF(DAY, P.DUEDATE, GETDATE()) BETWEEN 61 AND 90 THEN P.DUEAMOUNT- P.PAYMENT_OC ELSE 0 END AS DAYS90,
  CASE WHEN DATEDIFF(DAY, P.DUEDATE, GETDATE()) &gt; 90 THEN P.DUEAMOUNT- P.PAYMENT_OC ELSE 0 END AS DAYS91,
  CASE WHEN DATEDIFF(DAY, NT.ADATE, GETDATE()) BETWEEN 0 AND 30 THEN P.DUEAMOUNT- P.PAYMENT_OC ELSE 0 END AS NT_DAYS30,
  CASE WHEN DATEDIFF(DAY, NT.ADATE, GETDATE()) BETWEEN 31 AND 60 THEN P.DUEAMOUNT- P.PAYMENT_OC ELSE 0 END AS NT_DAYS60,
  CASE WHEN DATEDIFF(DAY, NT.ADATE, GETDATE()) BETWEEN 61 AND 90 THEN P.DUEAMOUNT- P.PAYMENT_OC ELSE 0 END AS NT_DAYS90,
  CASE WHEN DATEDIFF(DAY, NT.ADATE, GETDATE()) &gt; 90 THEN P.DUEAMOUNT- P.PAYMENT_OC ELSE 0 END AS NT_DAYS91, ADD1_DNV.VAL AS DN_PLCSYS1, ADD2_DNV.VAL AS DN_PLCSYS2, PLC.PLCID, NT.NOTANO
FROM FAPAYMENT P
INNER JOIN FAVOUCHER VO ON VO.VOUCHER = P.VOUCHER
INNER JOIN PLCNOTA NT ON VO.NOTANO = NT.NOTANO
INNER JOIN PLACING PLC ON PLC.PLCID = NT.PLCID
LEFT OUTER JOIN PLCINSURANCE INS ON NT.PLCID = INS.PLCID AND INS.TYPEF = 'L'
LEFT OUTER JOIN PLCADDINFO ADD1 ON NT.PLCID = ADD1.PLCID AND ADD1.CONFIGID = 'POLICY_NO'
LEFT OUTER JOIN PLCADDINFO ADD2 ON NT.PLCID = ADD2.PLCID AND ADD2.CONFIGID = 'PS_PIC_MKT'
LEFT OUTER JOIN CO ON CO.USRID = ADD2.VAL
LEFT OUTER JOIN PLCADDINFO ADD1_DNV ON NT.PLCID = ADD1_DNV.PLCID AND ADD1_DNV.CONFIGID = 'DN_VISION'
LEFT OUTER JOIN PLCADDINFO ADD2_DNV ON NT.PLCID = ADD2_DNV.PLCID AND ADD2_DNV.CONFIGID = 'DN_VISION2'
WHERE NT.DCTYPE = 'BB'</CommandText>
